# User-specific table sources for alice

# Orders table - alice's personal data source
tables {
  name: "orders"
  csv_file: "orders.csv"
  domains: "demo"
  columns {
    name: "status"
    display_name: "Status"
    type: COLUMN_TYPE_STRING
  }
  columns {
    name: "region"
    display_name: "Region"
