    type: COLUMN_TYPE_STRING
  }
  columns {
    name: "category"
    display_name: "Category"
    entity_type: "category"
    type: COLUMN_TYPE_STRING
  }
  columns {
    name: "amount"
    display_name: "Amount"
    type: COLUMN_TYPE_UINT32
  }
}
